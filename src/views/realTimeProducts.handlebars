<div class="container-fluid">
    <a href="/"><button class="btn btn-primary">Home</button></a>
    <h1>realTimeProducts</h1>
    <h2>Productos realTimeProducts</h2>
   <table class="table table-striped table-primary">
    <thead>
        <tr>
        <th >#id</th>
        <th >title</th>
        <th >description</th>
        <th >thumbnail</th>
        <th >code</th>
        <th >stock</th>
        <th >status</th>
        <th >price</th>
        <th >borrar</th>

        </tr>
    </thead>
    <tbody id="product_table">
    {{#each products}}
    
        <tr id="{{id}}">
        <th scope="row" >{{id}}</th>
        <th >{{title}}</th>
        <th >{{description}}</th>
        <th ><img class="imagen" src="{{thumbnail}}" alt=""></th>
        <th >{{code}}</th>
        <th >{{stock}}</th>
        <th >{{status}}</th>
        <th >{{price}}</th>
        <th ><button onclick="deleteProduct({{id}})" class="btn btn-danger">X</button></th>

        </tr>
        
   
    {{/each}}
    </tbody>
</table>
</div>
<div class="container card p-4">
    <h3>Ingresa un producto</h3>
    <form id="formAddProduct">
  <div class="mb-3">
    <label for="titulo" class="form-label">Titulo</label>
    <input id="titulo" class="form-control"  aria-describedby="emailHelp">
  </div>
 <div class="mb-3">
    <label for="descripcion" class="form-label">Descripci√≥n</label>
    <input id="descripcion" class="form-control"  aria-describedby="emailHelp">
  </div>
   <div class="mb-3">
    <label for="precio" class="form-label">Precio</label>
    <input id="precio" class="form-control"  aria-describedby="emailHelp">
  </div>
   <div class="mb-3">
    <label for="imagen" class="form-label">Imagen</label>
    <input id="imagen" class="form-control"  aria-describedby="emailHelp">
  </div>
   <div class="mb-3">
    <label for="code" class="form-label">Code</label>
    <input id="code" class="form-control"  aria-describedby="emailHelp">
  </div>
   <div class="mb-3">
    <label for="stock" class="form-label">Stock</label>
    <input id="stock" class="form-control"  aria-describedby="emailHelp">
  </div>
   <div class="mb-3">
    <label for="status" class="form-label">Status</label>
    <input id="status" class="form-control"  aria-describedby="emailHelp">
  </div>
  <button onclick="" id="submit" class="btn btn-primary">Submit</button>
</form>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>